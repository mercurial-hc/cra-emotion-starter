{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@reach/router": "^1.3.4",
    "@reduxjs/toolkit": "^1.4.0",
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/react": "^10.4.9",
    "@testing-library/user-event": "^11.2.1",
    "emotion-theming": "^10.0.27",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-scripts": "3.4.1",
    "typescript": "~3.9.6"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build:analyze": "yarn build --analyze",
    "build": "react-app-rewired build",
    "test": "cross-env CI=true yarn test:watch --coverage --env=jsdom",
    "test:watch": "react-app-rewired test",
    "postinstall": "yarn build && yarn test",
    "eject": "react-app-rewired eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@types/eslint": "^7.2.0",
    "@types/jest": "^26.0.8",
    "@types/node": "^14.6.2",
    "@types/reach__router": "^1.3.5",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.7",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.4.0",
    "babel-plugin-emotion": "^10.0.33",
    "cross-env": "^7.0.2",
    "customize-cra": "^1.0.0",
    "eslint": "^6.6.0",
    "eslint-plugin-react": "^7.20.0",
    "husky": ">=4",
    "lint-staged": ">=10",
    "prettier": "^2.0.5",
    "react-app-rewired": "^2.1.6",
    "tailwindcss": "^1.7.6",
    "twin.macro": "^1.7.0",
    "typescript-plugin-tw-template": "^2.0.1",
    "webpack-bundle-analyzer": "^4.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn build && yarn test && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "babelMacros": {
    "twin": {
      "config": "tailwind.config.js",
      "preset": "emotion",
      "hasSuggestions": true
    }
  }
}
